<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:eegeo="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.eegeo.mapapi.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        eegeo:camera_target_latitude="37.770713"
        eegeo:camera_target_longitude="-122.466095">

        <Button
            android:id="@+id/bNavigationCoffee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/navigation_coffee"
            android:visibility="gone" />
    </com.eegeo.mapapi.MapView>

</android.support.constraint.ConstraintLayout>